{
	"name": "vidup",
	"version": "1.0.13",
	"description": "Synchronize a local directory of video files to remote streaming services.",
	"keywords": [
		"bunnycdn-stream",
		"bunnycdn",
		"cli",
		"npm-package",
		"sync",
		"upload",
		"video"
	],
	"homepage": "https://github.com/kitschpatrol/vidup",
	"bugs": "https://github.com/kitschpatrol/vidup/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kitschpatrol/vidup.git"
	},
	"license": "MIT",
	"author": {
		"name": "Eric Mika",
		"email": "eric@ericmika.com",
		"url": "https://ericmika.com"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/lib/index.js",
			"types": "./dist/lib/index.d.ts"
		}
	},
	"main": "./dist/lib/index.js",
	"module": "./dist/lib/index.js",
	"types": "./dist/lib/index.d.ts",
	"bin": {
		"vidup": "dist/bin/cli.js"
	},
	"files": [
		"dist/*"
	],
	"scripts": {
		"build": "pkgroll --clean-dist --tsconfig tsconfig.build.json",
		"clean": "git rm -f pnpm-lock.yaml ; git clean -fdX",
		"fix": "ksc fix",
		"lint": "ksc lint",
		"release": "bumpp --commit 'Release: %s' && pnpm run build && pnpm publish --otp $(op read 'op://Personal/Npmjs/one-time password?attribute=otp')"
	},
	"dependencies": {
		"@types/yargs": "^17.0.33",
		"bunnycdn-stream": "^2.5.5",
		"execa": "^9.6.0",
		"ffmpeg-static": "^5.2.0",
		"hasha": "^7.0.0",
		"ora": "^9.0.0",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@kitschpatrol/shared-config": "^5.7.1",
		"@types/node": "^20.19.19",
		"bumpp": "^10.2.3",
		"lowdb": "^7.0.1",
		"picocolors": "^1.1.1",
		"pkgroll": "^2.16.0",
		"pretty-ms": "^9.3.0",
		"typescript": "^5.9.3",
		"untildify": "^6.0.0"
	},
	"packageManager": "pnpm@10.18.0",
	"engines": {
		"node": ">=20.19.0"
	},
	"publishConfig": {
		"access": "public"
	}
}

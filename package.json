{
	"name": "vidup",
	"version": "1.0.15",
	"description": "Synchronize a local directory of video files to remote streaming services.",
	"keywords": [
		"bunnycdn-stream",
		"bunnycdn",
		"cli",
		"npm-package",
		"sync",
		"upload",
		"video"
	],
	"homepage": "https://github.com/kitschpatrol/vidup",
	"bugs": "https://github.com/kitschpatrol/vidup/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kitschpatrol/vidup.git"
	},
	"license": "MIT",
	"author": {
		"name": "Eric Mika",
		"email": "eric@ericmika.com",
		"url": "https://ericmika.com"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/lib/index.d.ts",
			"import": "./dist/lib/index.js"
		}
	},
	"main": "./dist/lib/index.js",
	"module": "./dist/lib/index.js",
	"types": "./dist/lib/index.d.ts",
	"bin": {
		"vidup": "dist/bin/cli.js"
	},
	"files": [
		"dist/*"
	],
	"scripts": {
		"build": "pkgroll --clean-dist --tsconfig tsconfig.build.json",
		"clean": "git rm -f pnpm-lock.yaml ; git clean -fdX",
		"fix": "ksc fix",
		"lint": "ksc lint",
		"release": "bumpp --commit 'Release: %s' && pnpm run build && NPM_AUTH_TOKEN=$(op read 'op://Personal/npm/token') && pnpm publish"
	},
	"dependencies": {
		"@types/yargs": "^17.0.35",
		"bunnycdn-stream": "^2.5.5",
		"execa": "^9.6.1",
		"ffmpeg-static": "^5.3.0",
		"hasha": "^7.0.0",
		"ora": "^9.0.0",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@kitschpatrol/shared-config": "^5.10.0",
		"@types/node": "^20.19.25",
		"bumpp": "^10.3.2",
		"lowdb": "^7.0.1",
		"mdat-plugin-cli-help": "^1.0.4",
		"picocolors": "^1.1.1",
		"pkgroll": "^2.21.4",
		"pretty-ms": "^9.3.0",
		"typescript": "^5.9.3",
		"untildify": "^6.0.0"
	},
	"packageManager": "pnpm@10.25.0",
	"engines": {
		"node": ">=20.19.0"
	}
}
